    },
    "graphsearchTrustedValidationBulkReply": {
      "type": "object",
      "properties": {
        "valid": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/graphsearchTrustedValidationReply"
          }
        }
      }
    },
    "graphsearchTrustedValidationBulkRequest": {
      "type": "object",
      "properties": {
        "txids": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/graphsearchTrustedValidationRequest"
          }
        }
      }
    }
  }
}
