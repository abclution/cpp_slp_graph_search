      "type": "object",
      "properties": {
        "txdata": {
          "type": "array",
          "items": {
            "type": "string",
            "format": "byte"
          }
        }
      }
    },
    "graphsearchGraphSearchRequest": {
      "type": "object",
      "properties": {
        "txid": {
          "type": "string"
        },
        "exclude_txids": {
          "type": "array",
